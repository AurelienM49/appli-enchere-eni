<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" version="4.0">

  <display-name>appli-enchere-eni</display-name>

<!-- Déclaration de la jsp "acceuilLoged.jsp" -->
<servlet>
	<servlet-name>acceuilLoged</servlet-name>
	<jsp-file>/WEB-INF/jsp/acceuilLoged.jsp</jsp-file>
</servlet>

<!-- Association de la jsp "acceuilLoged.jsp" à une url -->
<servlet-mapping>
		<servlet-name>acceuilLoged</servlet-name>
		<url-pattern>/Home</url-pattern>
</servlet-mapping>

</web-app>